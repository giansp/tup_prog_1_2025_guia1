Proceso consumolitros
	Definir kilometros, litros, consumo Como Real;
	Escribir 'Ingrese los kilometros recorridos';
	Leer kilometros;
	Escribir 'Ingrese los litros consumidos';
	Leer litros;
	consumo <- litros/kilometros;
	Escribir 'El consumo de combustible por km es de: ', consumo, ' litros/km';
FinProceso
